{
  "name": "user-authentication-api",
  "version": "1.0.0",
  "description": "API for protecting routes with JWT authentication",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "prod": "npm run webpack -- --env.mode production",
    "dev": "npm run webpack -- --env.mode development --hot",
    "dev:server": "npm run webpack-dev-server -- --env.mode development",
    "start": "node ./dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-234/user-authentication-api.git"
  },
  "keywords": [
    "api",
    "jest",
    "authentication",
    "jwt"
  ],
  "author": "Daniele",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniel-234/user-authentication-api/issues"
  },
  "homepage": "https://github.com/daniel-234/user-authentication-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.1",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^3.1.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
